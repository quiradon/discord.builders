hikari.impl.ContainerComponentBuilder(
<% if (data.comp.accent_color) { -%>
    accent_color=hikari.Color.from_hex_code(<%= "#" + Number(data.comp.accent_color).toString(16).padStart(6, '0').toUpperCase() %>),
<% } -%>
<% if (data.comp.spoiler) { -%>
    spoiler=True,
<% } -%>
    components=[
        <%_ data.comp.components.forEach(function(comp){ _%>
            <%_ %><%- indent(include('/hikari/components.py.ejs', {comp: comp}), 8); -%>,
        <%_ }); _%>
    ]
)<% _%>
